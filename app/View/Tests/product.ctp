<img src="data:image/jpeg;base64," class="img-fluid" alt="Responsive image">
<form method="post" class="form-horizontal"  action="process">
	<input type="hidden" name="description" value="Ground Coffee 40 oz"/>
	<input type="hidden" name="price" value="7.5"/>
	<label>Description: Ground Coffee 40 oz</label><br>
	<label>Price: $7.5</label><br>
	<label class="control-label" style="display: inline-block;">quantity:</label>
	<select name="quantity">
	  <option>1</option>
	  <option>2</option>
	  <option>3</option>
	  <option>4</option>
	  <option>5</option>
	</select>
	<div class="radio">
	  <label>
	    <input id="radio_paypal" type="radio" name="payment_method" id="optionsRadios1" value="paypal" checked>
	   Paypal
	  </label>
	</div>
	<div class="radio">
	  <label>
	    <input id="radio_credit" type="radio" name="payment_method" id="optionsRadios2" value="credit">
	    CreditCard
	  </label>
	</div>

  <div class="">
    <label for="Number">Card Number</label>
    <input type="number" class="" id="Number" name="card_number" placeholder="Number">
  </div>
  <div class="">
    <label for="ExpireMonth">Expire Month</label>
    <input type="number" class="" id="ExpireMonth" name="ExpireMonth" placeholder="ExpireMonth">
  </div>
  <div class="">
    <label for="ExpireYear">Expire Year</label>
    <input type="number" class="" id="ExpireYear" name="ExpireYear" placeholder="ExpireYear">
  </div>
  <div class="">
    <label for="Cvv2">Cvv2</label>
    <input type="number" class="" id="Cvv2" name="Cvv2" placeholder="Cvv2">
  </div>
  <div class="">
    <label for="FirstName">First Name</label>
    <input type="text" class="" id="FirstName" name="FirstName" placeholder="FirstName">
  </div>
  <div class="">
    <label for="BillingCountry">Billing Country</label>
    <input type="text" class="" id="BillingCountry" name="BillingCountry" placeholder="US">
  </div>
  <div class="">
    <label for="LastName">Last Name</label>
    <input type="text" class="" id="LastName" name="LastName" placeholder="LastName">
  </div>
  <button type="submit" class="btn btn-default">Buy</button>
</form>
